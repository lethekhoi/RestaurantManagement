<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.restaurantmanagement">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_dinner_table"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.FullScreen">
        <activity android:name=".OrderActivity.OrderActivity"></activity>
        <activity android:name=".PayActivity.PayFoodActivity" />
        <activity android:name=".RestaurantMenu.RestaurantMenuActivity" />
        <activity android:name=".TableManagement.TableActivity" />
        <activity android:name=".NextActivity" />
        <activity android:name=".SignUp" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>